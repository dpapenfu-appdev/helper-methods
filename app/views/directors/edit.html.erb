<h1>Edit director</h1>

<% @director.errors.full_messages.each do |message| %>
  <p style="color: red;"><%= message %></p>
<% end %>



<%= form_with(model: @director, method: :patch) do |form| %>

<div>
   
   <%= form.hidden_field :id, value: @director.id%>
</div>

<div>
   <%= form.label :name%>
    
    <%=form.text_field :name %>
 </div>
 


 <div>
    <%= form.label :bio %>
    <%= form.text_field :bio, rows: 3%> 
</div>

<div>
    <%= form.label :dob %>
    <%= form.text_field :dob%> 
</div>

<%= form.button %>  
<%end%>
